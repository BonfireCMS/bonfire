{
  "name": "bonfire",
  "version": "0.0.0",
  "description": "Small description for admin goes here",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "bower": "bower install --allow-root",
    "build": "ember build",
    "postinstall": "npm run bower && npm run build -- --environment production",
    "start": "ember server",
    "test": "ember t -f 'JSHint' -i"
  },
  "repository": "",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "ember-ajax": "0.7.1",
    "ember-cli": "2.5.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-babel": "^5.1.6",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-eslint": "1.3.0",
    "ember-cli-font-awesome": "1.5.0",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-mirage": "0.1.13",
    "ember-cli-mocha": "0.10.2",
    "ember-cli-moment-shim": "1.1.0",
    "ember-cli-release": "0.2.8",
    "ember-cli-sass": "5.3.1",
    "ember-cli-spinkit": "^1.0.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.5.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.5.1",
    "ember-moment": "6.1.0",
    "ember-resolver": "^2.0.3",
    "ember-truth-helpers": "1.2.0",
    "eslint-config-ember": "0.3.0",
    "eslint-config-unstandard": "^1.3.2",
    "loader.js": "^4.0.1"
  },
  "eslintConfig": {
    "extends": "unstandard",
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "consistent-return": 0,
      "no-implicit-coercion": 0,
      "no-invalid-this": 0,
      "max-len": [
        1,
        100
      ],
      "quote-props": [
        2,
        "as-needed"
      ]
    }
  }
}
